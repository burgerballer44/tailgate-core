{
    "name": "burger/tailgate",
    "description": "score prediction with friends",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Darnell Collins",
            "homepage": "https://darnellcollins.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ramsey/uuid": "^3.8",
        "buttercup/protects": "dev-master",
        "mathiasverraes/classfunctions": "1.*",
        "respect/validation": "^1.1",
        "burger/eventpublisher": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4" : {
            "Tailgate\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tailgate\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/burgerballer44/protects"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burgerballer44/event-publisher"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burgerballer44/classfunctions"
        }
    ]
}
