{
    "name": "burger/tailgate-core",
    "description": "score prediction with friends",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Darnell Collins",
            "homepage": "https://github.com/burgerballer44",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ramsey/uuid": "^4.2",
        "burger/aggregate": "dev-master",
        "burger/event-publisher": "dev-master",
        "burger/common": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4" : {
            "Tailgate\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tailgate\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/burgerballer44/common"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burgerballer44/event-publisher"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burgerballer44/aggregate"
        }
    ]
}
